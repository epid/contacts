<% form_for(@contact) do |@f| %>
  <%= @f.error_messages %>

  <p>
    <%= @f.label :name %><br />
    <%= @f.text_field :name %>
  </p>
  <p>
    <%= @f.label :street %><br />
    <%= @f.text_field :street %>
  </p>
  <p>
    <%= @f.label :zip %><br />
    <%= @f.text_field :zip %>
  </p>
  <%= render :partial => 'city_state_parish' %>
  <p>
    <%= @f.submit "Save" %>
  </p>
<% end %>
